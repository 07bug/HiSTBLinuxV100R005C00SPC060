#===============================================================================
# export variable
#===============================================================================
ifeq ($(CFG_HI_EXPORT_FLAG),)
    SDK_DIR := $(shell cd $(CURDIR)/../.. && /bin/pwd)
    include $(SDK_DIR)/base.mak
endif

#===============================================================================
# rules
#===============================================================================
.PHONY: all clean

all:
	$(AT)make -C drv
	$(AT)make -C reg

clean:
	$(AT)make -C drv clean
	$(AT)make -C reg clean
